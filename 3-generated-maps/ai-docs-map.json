{
  "services": {
    "Auth": {
      "openapi": null,
      "internalFunctions": "documentation/internalfunctions/Auth.yml",
      "events": "documentation/events/Auth.yml",
      "description": "Handles user authentication including sign-in, sign-up, sign-out, password reset, and auth state management via React context"
    },
    "Menu": {
      "openapi": null,
      "internalFunctions": "documentation/internalfunctions/Menu.yml",
      "events": "documentation/events/Menu.yml",
      "description": "Manages cafe menu categories, items, and customization groups from Firestore"
    },
    "Order": {
      "openapi": null,
      "internalFunctions": "documentation/internalfunctions/Order.yml",
      "events": "documentation/events/Order.yml",
      "description": "Handles order creation, retrieval, status updates, promo code validation, and shopping cart state management with AsyncStorage persistence"
    },
    "Location": {
      "openapi": null,
      "internalFunctions": "documentation/internalfunctions/Location.yml",
      "events": "documentation/events/Location.yml",
      "description": "Manages cafe location data, nearest-location search via haversine distance, and selected pickup location state"
    },
    "Firebase": {
      "openapi": "documentation/openapi/Firebase.yml",
      "internalFunctions": "documentation/internalfunctions/Firebase.yml",
      "events": "documentation/events/Firebase.yml",
      "description": "Mock Firebase abstraction layer providing in-memory Firestore, Auth, Cloud Functions, and FCM with reactive snapshot listeners and demo data seeding"
    },
    "ZSociety": {
      "openapi": null,
      "internalFunctions": "documentation/internalfunctions/ZSociety.yml",
      "events": "documentation/events/ZSociety.yml",
      "description": "Manages Z Society membership status via real-time Firestore whitelist listener and provides membership context to the app"
    },
    "Wordle": {
      "openapi": null,
      "internalFunctions": "documentation/internalfunctions/Wordle.yml",
      "events": "documentation/events/Wordle.yml",
      "description": "Daily word puzzle game logic including deterministic word selection, guess checking, keyboard status tracking, and word validation"
    },
    "Navigation": {
      "openapi": null,
      "internalFunctions": "documentation/internalfunctions/Navigation.yml",
      "events": "documentation/events/Navigation.yml",
      "description": "App navigation structure with root navigator, bottom tab navigator with conditional Z Society tabs, and stack navigators for each section"
    }
  },
